---
type Heading = {
  depth: number;
  slug: string;
  text: string;
};

type Props = {
  headings: Heading[];
};

const props: Props = Astro.props;
---

<div>
  <h2>Table of Contents</h2>
  <ul>
    {props.headings.map((heading) => (
      <li>
        <a href={`#${heading.slug}`}>{heading.text}</a>
      </li>
    ))}
  </ul>
</div>
